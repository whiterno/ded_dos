
Format MZ

Start:		push cs
		pop ds

      		mov ah, 9		; Функция ДОСа 9 выводит строку 
		mov dx, Msg		; Вычисление адреса msg
		int 21h 		; Прерывание и передача управления ДОСу
					; для того, чтобы ДОС вывел строку

		mov ax, 4c00h		; Выход и окончательная передача упра-
		int 21h 		; вления ДОСу.

Msg		db 'Hello$'
